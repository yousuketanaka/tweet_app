<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <h2>ユーザー一覧</h2>
    <ul class="users">
      <% @users.each do |user| %>
        <li>
          <%= gravatar_for user %>
          <%= link_to user.name, user %>
        </li>
      <% end %>
    </ul>
    <%= will_paginate %>
  </div>
</div>	